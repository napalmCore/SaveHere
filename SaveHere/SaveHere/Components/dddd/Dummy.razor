@page "/dummy"

<PageTitle>Dummy</PageTitle>

<MudStack>

  <MudPaper Class="px-5 pt-4 mx-5 mt-5" Elevation="5">

    <MudStack Row>

      <MudTextField @bind-Value="UrlText" Label="File URL" Variant="Variant.Outlined" Margin="Margin.Dense" HelperText="Enter The File URL To Upload To The Server" HelperTextOnFocus="true" Adornment="Adornment.End" AdornmentIcon="@Icons.Material.Filled.InsertLink" AdornmentColor="Color.Info" Clearable="true" Immediate="true" />

      <MudButton Variant="Variant.Filled" StartIcon="@Icons.Material.Filled.Add" Color="Color.Primary" Style="text-transform:none; height:40px; margin-top:0.5rem;" Size="Size.Small">Add</MudButton>

    </MudStack>

  </MudPaper>

  <MudPaper Class="pa-5 mx-5" Elevation="5">



  </MudPaper>

</MudStack>

@code {
  public string UrlText { get; set; }
}
